{
  "name": "cypress-e2e-testing",
  "version": "1.0.0",
  "description": "Complete testing of an app using cypress",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YgorPerez/cypress-e2e-testing.git"
  },
  "scripts": {
    "client": "cd client && npm start",
    "server": "cd server && npm run dev",
    "dev": "concurrently \"npm run client\" \"npm run server\"",
    "start": "concurrently \"npm run client\" \"node index.js\"",
    "cypress:open": "cd client && npm run cypress:start:wait -- \"cypress open\"",
    "cypress:run": "cd client && npm run cypress:start:wait -- \"cypress run\""
  },
  "keywords": [],
  "author": "Ygor Perez <perezygor@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/YgorPerez/cypress-e2e-testing/issues"
  },
  "homepage": "https://github.com/YgorPerez/cypress-e2e-testing#readme",
  "devDependencies": {
    "concurrently": "^7.1.0"
  }
}
